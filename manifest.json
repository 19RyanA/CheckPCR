{
  "manifest_version": 2,
  "name": "Check PCR",
  "version": "2.0.0",
  "icons": {
  "16": "icon_16.png",
  "32": "icon_32.png",
  "64": "icon_64.png",
    "128": "icon_128.png"
  },
  "permissions": [
     "https://webappsca.pcrsoft.com/",
     "https://athena.harker.org/"
  ],
  "app": {
    "launch": {
      "local_path": "new.html"
    }
  },
  "minimum_chrome_version": "28",
  "offline_enabled": true,
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'"
}
